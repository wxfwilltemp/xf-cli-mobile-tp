root = true #表示是最顶层的配置文件，发现设为true时，才会停止查找.editorconfig文件

[*]   #匹配所有字符 对所有文件生效
charset = utf-8  #编码格式
max_line_length = 120 #指定120个字符后强制换行
indent_style = space # 缩进风格
indent_size = 2   #缩进的宽度
end_of_line = lf  #换行符
insert_final_newline = true #文件以一个空白行结尾
trim_trailing_whitespace = true #去除换行行首的任意空白字符
quote_type = single  #double=双引号 single=单引号 
